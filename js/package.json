{
    "name": "@tenet/core",
    "version": "0.1.0",
    "description": "Declarative logic VM for JSON schemas - reactive validation, temporal routing, and computed state",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "wasm"
    ],
    "scripts": {
        "build:wasm": "cd .. && GOOS=js GOARCH=wasm go build -o js/wasm/tenet.wasm ./wasm",
        "build:js": "tsc && node scripts/bundle.js",
        "build": "npm run build:wasm && npm run build:js",
        "test": "node --test dist/*.test.js",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "json-logic",
        "validation",
        "reactive",
        "schema",
        "form",
        "wasm",
        "compliance",
        "temporal"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/tenet"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/node": "^25.0.9",
        "typescript": "^5.0.0"
    }
}
